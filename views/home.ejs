							<input type="submit" class="Adicionar" value="Adicionar" name="ALT"<%if(acesso.veiculo_cliente!=1){%>disabled style='color:red '<%}%>></input>
							<input type="submit" class="Busca" value="Busca" name="BALT"></input>
						</div>
					</div>

				<div class="dropdown">
					<input type="submit"  name= "CF" class="main" value = "Funcionário" <%if(acesso.funcionario!=1){%>disabled style='color:red '<%}%>></input>
					<div class="dropdown-content">
						<input type="submit" class="Adicionar" value="Adicionar" name="ACF"<%if(acesso.funcionario!=1){%>disabled style='color:red '<%}%>></input>
						<input type="submit" class="Busca" value="Busca" name="BCF"<%if(acesso.funcionario!=1){%>disabled style='color:red '<%}%>></input>
						<input type="submit" class="BuscaInter" value="Busca CPF" name="BIF"<%if(acesso.funcionario!=1){%>disabled style='color:red '<%}%>></input>
					</div>
				</div>

					<div class="dropdown">
						<input type="submit"  class="main" value = "Rendimentos" <%if(acesso.producao_funcionario!=1&&acesso.lucro!=1){%>disabled style='color:red '<%}%>></input>
						<div class="dropdown-content">
							<input type="submit" class="Busca" value="Comissões" name="COM" <%if(acesso.producao_funcionario!=1){%>disabled style='color:red '<%}%>></input>
							<input type="submit" class="Busca" value="Relatórios" name="LC"<%if(acesso.lucro!=1){%>disabled style='color:red '<%}%>></input>
						</div>	
					</div>
	
				
				<div class="dropdown">
					<input type="submit"  name= "VD" class="main" value = "Venda de Produtos" <%if(acesso.venda!=1){%>disabled style='color:red '<%}%>></input>		
				</div>
				
			
				<div class="dropdown">
					<input type="submit"  name= "CAR" class="main" value = "Cargos" <%if(cargo!='PROPRIETARIO'){%>disabled style='color:red '<%}%>></input>
					<div class="dropdown-content">
						<input type="submit" class="Adicionar" value="Adicionar" name="CAR"<%if(cargo!='PROPRIETARIO'){%>disabled style='color:red '<%}%>></input>
						<input type="submit" class="Busca" value="Busca" name="BCAR"<%if(cargo!='PROPRIETARIO'){%>disabled style='color:red '<%}%>></input>
					</div>
				</div>	


				<div class="dropdown">
					<input type="submit" class="main" value = "Lembretes" name="Lembretes" <%if(acesso.conta!=1 && acesso.agendamento!=1 && acesso.evento!=1){%>disabled style='color:red '<%}%>></input>
				</div>
				
				<div class="dropdown">
						<input type="submit" class="sair" value = "Sair" name="sair" ></input>
					</div>
			</form>
	</div>
	</sript>
	</body>
	
	
</html>