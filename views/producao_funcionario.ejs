<html>
    <head>
        <meta charset="utf-8"/>
        <title>Produção Funionário</title>
        <link rel="stylesheet" type="text/css" href="css/buscaProducao.css">
    </head>

    <body>
        <br>
        	<form autocomplete="off" action='/red' method='get'>
                <h1><input type="submit" class="HOME" value="Início" name="HOME"></input></h1>
            </form>
        <h2>Comissão do Funcionário: <%=funcionario.nome%></h2>
        <h3>Porcentagem de comissão: <%=comissao%>%</h3>
        <h3>Salário: R$<%=funcionario.salario%></h3> 
        <h3>Comissão: R$<%=total%></h3>

        <table id="tabela_producao">
            
            <%if(OS.length>0){%>
                <tr>
                    <th id="producaoOS">Id da Ordem de Serviço</th>
                    <th>Mão de obra</th>
                </tr>
                
                <% for(var I = 0; I<OS.length; I++){ %>
                    <tr>
                        <td class="producao"><%=OS[I].id%></td>
                        <td class="producao"><%=OS[I].preco%></td>
                    </tr>
                <%}%>
            <%}%>
        </table>
    </body>
</html>